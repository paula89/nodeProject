{
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "indent": [2], // ,3] to says qty spaces,
    "semi": [2],
    "quotes": [2],
    "comma-dangle": "off"
  }
}
